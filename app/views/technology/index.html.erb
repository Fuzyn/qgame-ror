<% if flash[:notice] %>
  <div class="success-messages">
    <p><%= flash[:notice] %></p>
  </div>
<% elsif flash[:alert] %>
  <div class="error-messages">
    <p><%= flash[:alert] %></p>
  </div>
<% end %>

<% @technology_queue.each do |element| %>
  <%= render partial: "common/queue", locals: { element: element, path: technology_path } %>
<% end %>

<% @user_technology.each do |technology| %>
  <%= render partial: "technology/technology", locals: { technology: technology } %>
<% end %>
