<% if flash[:notice] %>
  <div class="success-messages">
    <p><%= flash[:notice] %></p>
  </div>
<% elsif flash[:alert] %>
  <div class="error-messages">
    <p><%= flash[:alert] %></p>
  </div>
<% end %>

<% @building_queue.each do |element| %>
  <%= render partial: "common/queue", locals: { element: element, path: building_path } %>
<% end %>

<% @planet_buildings.each do |building| %>
  <%= render partial: "building", locals: { building: building } %>
<% end %>
