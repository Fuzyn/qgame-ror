<% if flash[:notice] %>
  <div class="success-messages">
    <p><%= flash[:notice] %></p>
  </div>
<% elsif flash[:alert] %>
  <div class="error-messages">
    <p><%= flash[:alert] %></p>
  </div>
<% end %>

<% @planet_defence.each do |element| %>
  <%= render partial: "defence/cannon", locals: { element: element } %>
<% end %>